{
    "extends": [
      "standard"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "no-multiple-empty-lines": "off",
      "quote-props": [
        1,
        "consistent"
      ]
    },
    "env": {
      "browser": true
    }
  }
  